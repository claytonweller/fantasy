    "react-scripts": "5.0.1",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "eval $(egrep -v '^#' .env | xargs) react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "npx prettier . --write",
    "load-snapshot": "bash ./src/data/snapshots/loadSnapshot.sh",
    "store-snapshot": "bash ./src/data/snapshots/storeSnapshot.sh",
    "scrub-research": "node_modules/.bin/tsx src/data/rawScripts/scrubResearch.ts && yarn format",
    "scrub-by-week": "node_modules/.bin/tsx src/data/rawScripts/runScrubByWeek.ts",
    "set-data-state": "node_modules/.bin/tsx src/data/rawScripts/runSetDataState.ts",
    "deploy": "bash ./deploy.sh"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
